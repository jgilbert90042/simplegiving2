{% for church in churches %}
<table class=messages>
<tr>
  <td colspan=2 class=messages>{{ church.church.name }}</td>
<tr>
<tr>
  <td class=messages>Denomination</td>
  <td class=messages> {{ church.church.denomination.name }}</td>
</tr>
<tr>
  <td class=messages>Address</td>
  <td class=messages> {{ church.church.address }}</td>
</tr>
<tr>
  <td class=messages>City</td>
  <td class=messages> {{ church.church.city }}</td>
</tr>
<tr>
  <td class=messages>State</td>
  <td class=messages> {{ church.church.state }}</td>
</tr>
<tr>
  <td class=messages>Zip</td>
  <td class=messages> {{ church.church.zip }}</td>
</tr>
<tr>
  <td class=messages>Phone</td>
  <td class=messages> {{ church.church.phone }}</td>
</tr>
{% if church.church.fax %}
<tr>
  <td class=messages>Fax</td>
  <td class=messages> {{ church.church.fax }}</td>
</tr>
{% endif %}
{% if church.church.email %}
<tr>
  <td class=messages>Email</td>
  <td class=messages> {{ church.church.email }}</td>
</tr>
{% endif %}
{% if church.church.website %}
<tr>
  <td class=messages>Website</td>
  <td class=messages> {{ church.church.website }}</td>
</tr>
{% endif %}
<tr>
  <td colspan=2 class=messages><a href="{{ path('_message_public_send', {'targetId': church.verifier.user.id}) }}">Message Verification Specialist</a></td>
</tr>
</table>
<br>
<br>
{% endfor %}
